				SuppressStartupBanner="TRUE"
				ModuleDefinitionFile=".\Src\jDoom\JDoom.def"
				ProgramDatabaseFile=".\./Bin/Release/JDoom.pdb"
				ImportLibrary=".\./Bin/Release/JDoom.lib"/>
